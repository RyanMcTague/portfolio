    "styled-components": "^5.3.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.1",
    "@types/node": "18.15.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "^5.1.26",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
